{
  "rewrites": [
    {
      "source": "/([0-9]{4})/([0-9]{2})/(.*)\\.html",
      "destination": "https://liuweinan.com/posts/$1/$2/$3",
      "has": [
        {
          "type": "host",
          "value": "blog.liuweinan.com"
        }
      ]
    },
    {
      "source": "/([0-9]{4})/([0-9]{2})",
      "destination": "https://liuweinan.com/posts/$1/$2",
      "has": [
        {
          "type": "host",
          "value": "blog.liuweinan.com"
        }
      ]
    },
    {
      "source": "/([0-9]{4})",
      "destination": "https://liuweinan.com/posts/$1",
      "has": [
        {
          "type": "host",
          "value": "blog.liuweinan.com"
        }
      ]
    },
    {
      "source": "/",
      "destination": "https://liuweinan.com/posts",
      "has": [
        {
          "type": "host",
          "value": "blog.liuweinan.com"
        }
      ]
    },
    {
      "source": "/category/(.*)",
      "destination": "https://liuweinan.com/categories/$1",
      "has": [
        {
          "type": "host",
          "value": "blog.liuweinan.com"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.liuweinan.com/Picture/",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "spaces.liuweinan.com"
        }
      ]
    }
  ]
}
